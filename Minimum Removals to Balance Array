class Solution {
    public int minRemoval(int[] nums, int k) {
        Arrays.sort(nums);
        int n=nums.length;
        int ans=Integer.MAX_VALUE;
        int right=0;
        for(int left=0;left<n;left++)
        {
            while(right<n && nums[right]<=(long)nums[left]*k )
            {
                right++;
            }
            ans=Math.min(ans,n-(right-left));
        }
        return ans;
    }
}
